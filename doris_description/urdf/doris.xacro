  <!-- right front wheel -->
	<link name="right_front_wheel">
		<inertial>
			<mass value="1.5"/>
			<origin xyz="0.0 -0.04 0.00"/>
			<inertia ixx="0.0051" ixy="0.0" ixz="0.0"
				 iyy="0.0051" iyz="0.0"
				 izz="0.009"/>
		</inertial>
		<visual name="base_visual">
			<origin xyz="0.0 -0.04 0.00" rpy="1.5707 1.5707 0"/>
			<geometry>
				<mesh filename="package://doris_description/urdf/meshes/wheel.dae"/>
			</geometry>			
		</visual>
		<collision>
			<origin xyz="0.0 -0.04 0.00" rpy="1.5707 1.5707 0"/>
				<geometry>
					<mesh filename="package://doris_description/urdf/meshes/wheel.dae"/>
				</geometry>
		</collision>
	</link>

  <joint name="right_front_wheel_hinge" type="continuous">
    <parent link="base_link"/>
    <child link="right_front_wheel"/>
    <origin xyz="0.13 -0.17 0.11" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <anchor xyz="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>


