  <!-- Chassis -->
	<link name="base_link">
		<inertial>
			<mass value="14.0"/>
			<origin xyz="0 0 0.18"/>
			<inertia ixx="0.147" ixy="0.0" ixz="0.0"
				 iyy="0.3349" iyz="0.0"
				 izz="0.3978"/>
		</inertial>
		<visual name="base_visual">
			<origin xyz="0 0 0.18" rpy="0 0 0"/>
			<geometry>
